    "chai": "^5.2.1",
    "concurrently": "^8.2.2",
    "ethers": "^6.15.0",
    "hardhat": "^2.26.3",
    "typechain": "^8.3.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "ansi-regex": "^5.0.1",
    "ansi-styles": "^4.3.0",
    "axios": "^1.11.0",
    "chalk": "^4.1.2",
    "cliui": "^8.0.1",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
