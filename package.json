{
  "name": "defi-lending-platform",
  "version": "1.0.0",
  "description": "DeFi lending platform with credit scoring and referral system",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && node server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:hardhat": "npx hardhat node",
    "dev:full": "concurrently \"npm run dev:hardhat\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "hardhat:start": "node scripts/startHardhatNetwork.js",
    "demo:setup": "concurrently \"npm run dev:hardhat\" \"sleep 5 && npm run dev:backend\" \"sleep 10 && npm run dev:frontend\""
  },
  "keywords": [
    "defi",
    "lending",
    "credit-scoring",
    "blockchain",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
